{
  "name": "hono-rate-limiter",
  "description": "Rate limit middleware for Hono.",
  "version": "0.5.0",
  "type": "module",
  "license": "MIT",
  "keywords": [
    "hono",
    "api",
    "middleware",
    "rest-api",
    "rate-limiting",
    "rate-limiter",
    "honojs"
  ],
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": ["dist"],
  "scripts": {
    "build": "pkgroll --clean-dist",
    "test": "vitest",
    "prepare": "is-ci || husky",
    "format": "biome check --write ."
  },
  "exports": {
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    }
  },
  "homepage": "https://hono-rate-limiter.vercel.app",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rhinobase/hono-rate-limiter.git"
  },
  "bugs": {
    "url": "https://github.com/rhinobase/hono-rate-limiter/issues"
  },
  "peerDependencies": {
    "hono": "^4.1.1",
    "unstorage": "^1.17.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.7.3",
    "@types/node": "20",
    "husky": "^9.1.7",
    "is-ci": "^3.0.1",
    "nano-staged": "^0.8.0",
    "typescript": "~5.4.5",
    "vitest": "^1.6.0"
  }
}
